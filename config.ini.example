[MAIN]
# Use local LLM provider (True/False)
is_local = False

# LLM Provider: anthropic, openai, ollama, google, etc.
provider_name = anthropic

# Model name (examples: claude-sonnet-4-20250514, gpt-4, llama3, etc.)
provider_model = claude-sonnet-4-20250514

# Server address for local providers (leave empty for cloud providers)
provider_server_address =

# Agent name
agent_name = Jarvis

# Recover last session on startup (True/False)
recover_last_session = False

# Save session after each interaction (True/False)
save_session = True

# Enable text-to-speech (True/False)
speak = False

# Enable speech-to-text (True/False)
listen = False

# Use Jarvis personality (True/False) - affects prompts and behavior
jarvis_personality = True

# Supported languages (space separated)
languages = en ru

[BROWSER]
# Run browser in headless mode (True/False)
headless_browser = True

# Enable stealth mode for browser (True/False)
stealth_mode = False

# Note: Set API keys and secrets in environment variables:
# - ANTHROPIC_API_KEY for Anthropic Claude
# - OPENAI_API_KEY for OpenAI GPT
# - GOOGLE_API_KEY for Google Gemini
# etc.